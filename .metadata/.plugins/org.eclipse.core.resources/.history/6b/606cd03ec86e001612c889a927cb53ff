public class temperature {
	public static void main(String[]args) {
		double f;
		double c;
		double convTemp;
		
		f = 105;
		c = (5/9) * (f-32);
		convTemp = c;
		
		System.out.println("converted temperature is");
		System.out.println(convTemp);			
	}
}
